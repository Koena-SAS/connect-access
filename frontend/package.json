{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^2.13.1",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.3",
    "@lingui/react": "^3.13.2",
    "@material-ui/core": "^5.0.0-beta.5",
    "@material-ui/icons": "^5.0.0-beta.5",
    "@material-ui/lab": "^5.0.0-alpha.44",
    "axios": "^0.26.0",
    "framer-motion": "^6.2.6",
    "immer": "^9.0.12",
    "little-state-machine": "^4.2.3",
    "little-state-machine-devtools": "^2.0.1",
    "lodash": "^4.17.21",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.15.8",
    "react-markdown": "^8.0.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-slugify": "^2.1.0",
    "swr": "^0.5.7",
    "use-mutation": "^2.2.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "lingui compile && react-app-rewired build && mv build ../backend/staticfiles/connect_access_bundles",
    "test": "react-app-rewired test",
    "lint": "eslint --max-warnings=0 src",
    "lang:extract": "lingui extract",
    "lang:compile": "lingui compile",
    "e2e": "TS_NODE_PROJECT='./end_to_end/tsconfig.json' npx codeceptjs run --steps --config ./end_to_end/config/codecept_development.conf.js",
    "e2e:prod": "TS_NODE_PROJECT='./end_to_end/tsconfig.json' npx codeceptjs run --steps --config ./end_to_end/config/codecept_production.conf.js",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:e2e": "tsc -p end_to_end/tsconfig.json --noEmit",
    "test-all": "yarn lint && yarn type-check && yarn type-check:e2e && yarn test --watchAll=false",
    "build:svg": "svgr src/images/svg/ -d src/images/buildSvg/ --typescript",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@axe-core/react": "^4.4.0",
    "@codeceptjs/configure": "^0.8.0",
    "@lingui/cli": "^3.13.2",
    "@lingui/core": "^3.13.2",
    "@lingui/macro": "^3.13.2",
    "@svgr/cli": "^6.2.1",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "axe-playwright": "^1.1.9",
    "codeceptjs": "^3.3.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "faker": "^5.5.3",
    "playwright": "^1.19.1",
    "pm2": "^5.2.0",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "react-app-rewired": "^2.2.1",
    "rosie": "^2.1.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "weak-napi": "^2.0.2",
    "web-vitals": "^2.1.4",
    "webpack-bundle-tracker": "^0.4.3"
  }
}
